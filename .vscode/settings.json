{
    "json.schemas": [
        {
            "fileMatch": [
                "**/palettes/RainStates*.json"
            ],
            "url": "./palette.schema.json"
        }
    ],
    "json.validate.enable": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.formatOnSave": true,
        "editor.tabSize": 4
    }
}
